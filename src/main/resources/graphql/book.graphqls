type Book {
    id: ID!
    name: String
    isbn: String
    author: Author
    publisher: String
    createdAt: Long
    lastModified: Long
}

extend type Query {
    findAllBooks: [Book]!
    countBooks: Long!
}

extend type Mutation {
  newBook(isbn: String,phoneNum String) : Book!
  deleteBook(id: ID!) : Boolean
  updateBookPageCount(pageCount: Int!, id: ID!) : Book!
}
